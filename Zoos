#include <iostream>
using namespace std;

// main() is where program execution begins.
int main()
{
char a[20];
cin >> a; 
    



int b=0;
int c=0;
int i;
for(i=0;i<20;i++)
{
    if(a[i]=='z')
    {
        b=b+1;
    }
    else if(a[i]=='o')
    {
    c=c+1;
    }
}

if(2*b==c)
    {
    cout<<"Yes";
    }
else
{
cout<<"No";
}
}
